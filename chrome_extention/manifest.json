{
    "name": "Job ID Extractor",
    "version": "1.0",
    "description": "allows users to check their compatibility with job",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "index.html",
      "default_title": "Job ID Extractor",
      "popup": "index.html"
    }
  }
  